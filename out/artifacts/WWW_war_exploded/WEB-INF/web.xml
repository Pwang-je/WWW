<?xml version="1.0" encoding="UTF-8"?>
<!--suppress ALL -->
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>posttest</servlet-name>
        <servlet-class>PostData</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>posttest</servlet-name>
        <url-pattern>/PostData</url-pattern>
        <url-pattern>/SessionTest</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>shop_main</servlet-name>
        <servlet-class>Shopping.Cart</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>shop_main</servlet-name>
        <url-pattern>/Cart</url-pattern>
        <url-pattern>/ServletDb</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>report_main</servlet-name>
        <servlet-class>Report.Insert</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>report_main</servlet-name>
        <url-pattern>/Insert</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>bang_main</servlet-name>
        <servlet-class>Bang.BangServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>bang_main</servlet-name>
        <url-pattern>/BangServlet</url-pattern>
    </servlet-mapping>

</web-app>